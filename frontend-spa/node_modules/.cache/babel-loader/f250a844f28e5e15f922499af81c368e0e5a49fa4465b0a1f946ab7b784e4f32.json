{"ast":null,"code":"import { mapState, mapActions } from 'vuex';\nexport default {\n  name: \"PesquisaCep\",\n  data() {\n    return {\n      cep: \"\"\n    };\n  },\n  computed: {\n    ...mapState({\n      endereco: state => state.endereco,\n      error: state => state.error,\n      loading: state => state.loading\n    })\n  },\n  methods: {\n    ...mapActions(['pesquisarCEP']),\n    atualizarCep() {\n      this.$store.commit('setCep', this.cep);\n    },\n    pesquisar() {\n      this.pesquisarCEP(this.cep);\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapActions","name","data","cep","computed","endereco","state","error","loading","methods","atualizarCep","$store","commit","pesquisar","pesquisarCEP"],"sources":["C:\\teste\\frontend-spa\\src\\views\\Pesquisa.vue"],"sourcesContent":["<template>\r\n    <div class=\"pesquisa\">\r\n      <div class=\"card\">\r\n        <h2>Pesquisa de CEP</h2>\r\n        <input type=\"text\" v-model=\"cep\" @input=\"atualizarCep\" placeholder=\"Digite o CEP\" />\r\n        <button @click=\"pesquisar\">Pesquisar</button>\r\n  \r\n        <div v-if=\"loading\">Carregando...</div>\r\n        <div v-else-if=\"error\" class=\"error\">{{ error }}</div>\r\n  \r\n        <div v-if=\"endereco\" class=\"result-card\">\r\n          <h3>Resultados da pesquisa:</h3>\r\n          <p><strong>Logradouro:</strong> {{ endereco.logradouro }}</p>\r\n          <p><strong>Bairro:</strong> {{ endereco.bairro }}</p>\r\n          <p><strong>Localidade:</strong> {{ endereco.localidade }}</p>\r\n          <p><strong>UF:</strong> {{ endereco.uf }}</p>\r\n        </div>\r\n      </div>\r\n      <router-link to=\"/\">\r\n        <button>Voltar</button>\r\n      </router-link>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { mapState, mapActions } from 'vuex';\r\n  \r\n  export default {\r\n    name: \"PesquisaCep\",  \r\n    data() {\r\n      return {\r\n        cep: \"\",\r\n      };\r\n    },\r\n    computed: {\r\n      ...mapState({\r\n        endereco: state => state.endereco,\r\n        error: state => state.error,\r\n        loading: state => state.loading,\r\n      }),\r\n    },\r\n    methods: {\r\n      ...mapActions(['pesquisarCEP']),\r\n      atualizarCep() {\r\n        this.$store.commit('setCep', this.cep);\r\n      },\r\n      pesquisar() {\r\n        this.pesquisarCEP(this.cep);\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .pesquisa {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    flex-direction: column;\r\n    height: 100vh;\r\n    padding: 20px;\r\n  }\r\n  \r\n  .card {\r\n    background-color: #fff;\r\n    border-radius: 8px;\r\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n    padding: 1.5rem;\r\n    width: 100%;\r\n    max-width: 400px;\r\n    text-align: center;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  input {\r\n    width: 100%;\r\n    padding: 10px;\r\n    margin-bottom: 1rem;\r\n    border-radius: 4px;\r\n    border: 1px solid #ddd;\r\n  }\r\n  \r\n  button {\r\n    width: 100%;\r\n    padding: 10px;\r\n    background-color: #007bff;\r\n    color: white;\r\n    border: none;\r\n    cursor: pointer;\r\n    border-radius: 4px;\r\n    font-size: 16px;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #0056b3;\r\n  }\r\n  \r\n  .result-card p {\r\n    margin-bottom: 0.5rem;\r\n  }\r\n  \r\n  .error {\r\n    color: red;\r\n    margin-top: 1rem;\r\n    font-weight: bold;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAyBE,SAASA,QAAQ,EAAEC,UAAS,QAAS,MAAM;AAE3C,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE;IACP,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGL,QAAQ,CAAC;MACVM,QAAQ,EAAEC,KAAI,IAAKA,KAAK,CAACD,QAAQ;MACjCE,KAAK,EAAED,KAAI,IAAKA,KAAK,CAACC,KAAK;MAC3BC,OAAO,EAAEF,KAAI,IAAKA,KAAK,CAACE;IAC1B,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,GAAGT,UAAU,CAAC,CAAC,cAAc,CAAC,CAAC;IAC/BU,YAAYA,CAAA,EAAG;MACb,IAAI,CAACC,MAAM,CAACC,MAAM,CAAC,QAAQ,EAAE,IAAI,CAACT,GAAG,CAAC;IACxC,CAAC;IACDU,